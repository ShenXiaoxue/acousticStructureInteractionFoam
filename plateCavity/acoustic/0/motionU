FoamFile
{
    version     2.0;
    format      ascii;
    class       tetPointVectorField;
    object      motionU;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    Inlet
    {
        type            fixedValue;
	value uniform (0 0 0);
    }
    consoleAcoustic	
    {
        type            fixedValue;
	value uniform (0 0 0);
    }
    emission
    {
        type            fixedValue;
	value uniform (0 0 0);
    }
    reception
    {
        type            fixedValue;
	value uniform (0 0 0);
    }
}

